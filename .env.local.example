# Upstash Redis configuration (required)
UPSTASH_REDIS_REST_URL=https://your-upstash-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-upstash-redis-token

# Google Cloud Storage configuration (required)
GCS_BUCKET_NAME=your-gcs-bucket-name
# For local development only - path to service account key file
GOOGLE_APPLICATION_CREDENTIALS=./gcp-credentials.json

# Rate limiting configuration (optional)
UPSTASH_RATE_LIMIT_REQUESTS=10
UPSTASH_RATE_LIMIT_WINDOW=10 s

# Vercel Cron security (for production)
CRON_SECRET=your-secret-for-cron-jobs